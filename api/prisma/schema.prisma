generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("DATABASE_URL")
}

model Score {
  scoreID String @db.Uuid @id @default(uuid())
  userID String @db.Uuid
  modID String
  songID Int
  diffID Int
  time DateTime @default(now())
  score Int
  pass Boolean @default(true)
}
